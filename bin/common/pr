#!/bin/bash

# $1 - repo to check e.g. Snapchat/appengine
function get_pr {
    org=$(echo $1 | awk -F/ '{ print $1; }')
    curl -H "Authorization: token $(<$GITHUB_TOKEN)" \
         $GITHUB_API/repos/$1/pulls?head=$org:$BRANCH \
         2>/dev/null | jq -r "map(select(.head.ref == \"$BRANCH\")) | .[].number"
}
